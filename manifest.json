{
  "name": "Get Google Cache",
  "version": "1",
  "manifest_version": 2,
  "description": "Get Google's cached version of the current page in a new tab.",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Get Google Cache",
	"default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png",
    "256": "images/icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
